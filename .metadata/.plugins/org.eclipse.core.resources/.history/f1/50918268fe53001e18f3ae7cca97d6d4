import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();
		int i = 0;
		int count;
		
		for(i = 0; x > 0 && i <=100; i++) {
			count = i + i;
			System.out.println(count);
		}
		
		
		sc.close();

	}

}

